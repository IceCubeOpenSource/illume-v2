# Try to match interactive jobs to interactive-only slots
JOB_TRANSFORM_NAMES = $(JOB_TRANSFORM_NAMES) SetExp
JOB_TRANSFORM_SetExp @=end
[
        set_Rank = ifThenElse(InteractiveJob && InteractiveSlot, 1, 0)
]
@end

